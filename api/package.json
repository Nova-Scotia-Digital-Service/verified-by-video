{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "description": "Verified by video API",
  "packageManager": "yarn@4.4.0",
  "engines": {
    "node": ">=v20.11.0 <21.0.0",
    "yarn": "^4.4.0"
  },
  "main": "src/index.js",
  "scripts": {
    "create-submission": "ts-node ./scripts/create-submission.ts",
    "start": "nodemon --watch src -e ts,tsx --exec ts-node ./src/index.ts",
    "start:nowatch": "ts-node src/index.ts",
    "build": "rm -rf build && tsc",
    "serve": "node build/api/src/index.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/swagger": "^7.4.0",
    "express": "^4.19.2",
    "jose": "^5.7.0",
    "minio": "^8.0.1",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.6.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
